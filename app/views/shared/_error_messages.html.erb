<% if error_messages.errors.any? %>
<div id="error_explanation">
  <div class="alert alert-error">
    The form contains <%= pluralize(error_messages.errors.count, "error") %>
  </div>
  <ul>
    <% error_messages.errors.full_messages.each do |msg|%>
     <li>* <%= msg %></li>
    <% end %>
  </ul>
</div>
<% end %>
